{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: run app",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "src/rta_pvt/app_streamlit.py",
        "--server.port", "8503",
        "--server.headless", "true"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    }
  ]
}
